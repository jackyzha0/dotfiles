[user]
	email = j.zhao2k19@gmail.com
	name = Jacky Zhao
[pull]
	rebase = false
[push]
	default = current
[init]
	defaultBranch = main
[core]
	excludesfile = /Users/jzhao/.gitignore
	pager = diff-so-fancy | less --tabs=4 -R
[interactive]
	diffFilter = diff-so-fancy --patch
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = 11
	frag = magenta bold
	func = 146 bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[diff-so-fancy]
	markEmptyLines = false
